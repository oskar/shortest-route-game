{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "member-access": false,
    "interface-over-type-literal": false,
    "interface-name": {
      "options": ["never-prefix"]
    },
    "jsx-no-lambda": false,
    "no-console": {
      "severity": "warning"
    },
    "object-literal-sort-keys": false
  }
}
